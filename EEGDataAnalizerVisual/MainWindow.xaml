<Window x:Class="EEGDataAnalizerVisual.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:oxy="http://oxyplot.org/wpf"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EEGDataAnalizerVisual"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <oxy:PlotView Model="{Binding MyModel}" VerticalAlignment="Bottom" HorizontalAlignment="Center" Width="800" Height="400"/>
        <Button Content="Prev" HorizontalAlignment="Left" VerticalAlignment="Top" Click="Prev"/>
        <Button Content="Next" HorizontalAlignment="Right" VerticalAlignment="Top" Click="Next"/>
    </Grid>
</Window>
