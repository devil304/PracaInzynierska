<Window x:Class="EEGDataCollectorNative.End"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EEGDataCollectorNative"
        mc:Ignorable="d"
        Title="End" Height="450" Width="800"
        ResizeMode="NoResize">
    <StackPanel>
        <TextBlock Margin="0,100,0,0" Text="Dziękujemy za udział w serii testów, wpisz poniżej imię i ewentualne id ochotnika." VerticalAlignment="Center" HorizontalAlignment="Center" TextWrapping="Wrap" FontSize="20" MaxWidth="800" TextAlignment="Left"/>
        <TextBox x:Name="SubjectName" Margin="0,100,0,0" Width="400" Text="" VerticalAlignment="Center" HorizontalAlignment="Center" TextWrapping="Wrap" FontSize="20" MaxWidth="800" TextAlignment="Left" AutomationProperties.HelpText="SubjectName"/>
        <TextBlock x:Name="spaceHint" Margin="0,120,0,0" Text="Wciśnij spację aby zakończyć testy." VerticalAlignment="Bottom" HorizontalAlignment="Center" TextWrapping="Wrap" FontSize="20" MaxWidth="700" TextAlignment="Center" Focusable="True"/>
    </StackPanel>
</Window>
